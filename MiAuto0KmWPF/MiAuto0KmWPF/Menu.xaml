<Window x:Class="MiAuto0KmWPF.Menu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MiAuto0KmWPF"
        mc:Ignorable="d"
        Title="Menu" WindowState="Maximized">
    <DockPanel>
        <Menu Name="MenuPrincipal">
            <MenuItem Header="Options">
                <MenuItem Header="Contacto" IsEnabled="False"/>
                <MenuItem Header="Concesionaria" IsEnabled="False"/>
                <MenuItem Header="Vehiculo">
                    <MenuItem Header="Marcas" Click="MenuMarca_Click"/>
                    <MenuItem Header="Tipo de Vehiculo" IsEnabled="False"/>
                    <MenuItem Header="Tipo de Transmision" IsEnabled="False" />
                </MenuItem>
                <Separator />
                <MenuItem Header="Salir" />
            </MenuItem>
            <MenuItem Header="Edit"/>
            <MenuItem Header="View"/>
            <MenuItem Header="Help"/>
        </Menu>
    </DockPanel>
</Window>
